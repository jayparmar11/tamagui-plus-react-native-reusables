{
  "extends": "../../tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "composite": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx",
    "nativewind-env.d.ts",
    "types.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "babel.config.js"
  ]
}